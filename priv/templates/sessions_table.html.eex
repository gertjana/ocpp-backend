<table class="table table-sm table-striped">
  <thead class="thead-light">
    <th>Transaction Id</th>
    <th>Serial</th>
    <th>Connector</th>
    <th>Token</th>
    <th>Start Time</th>
    <th>Stop Time</th>
    <th>Volume</th>
    <th>Duration</th>
  </thead>
    <%= Enum.map(sessions, fn s -> %>
      <tr> 
        <td><%= s.transaction_id%></td>
        <td><%= s.serial%></td>
        <td><%= s.connector_id%></td>
        <td><%= s.token%></td>
        <td><%= s.start_time %></td>
        <td><%= s.stop_time %></td>
        <td><%= s.volume %></td>
        <td><%= s.duration %></td>
      </tr>
    <% end) %>
</table>
